{application,bureaucrat,
             [{applications,[kernel,stdlib,elixir,inflex,plug]},
              {description,"Generate Phoenix API documentation from tests"},
              {modules,['Elixir.Bureaucrat',
                        'Elixir.Bureaucrat.ApiBlueprintWriter',
                        'Elixir.Bureaucrat.Formatter',
                        'Elixir.Bureaucrat.Helpers','Elixir.Bureaucrat.JSON',
                        'Elixir.Bureaucrat.MarkdownWriter',
                        'Elixir.Bureaucrat.Recorder',
                        'Elixir.Bureaucrat.SwaggerSlateMarkdownWriter',
                        'Elixir.Bureaucrat.Util']},
              {vsn,"0.2.5"},
              {registered,['Elixir.Bureaucrat.Recorder']},
              {mod,{'Elixir.Bureaucrat',[]}},
              {env,[{writer,'Elixir.Bureaucrat.MarkdownWriter'},
                    {default_path,<<"web/controllers/README.md">>},
                    {paths,[]},
                    {titles,[]},
                    {env_var,<<"DOC">>}]}]}.
